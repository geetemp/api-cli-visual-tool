{
  "name": "api-cli",
  "version": "1.0",
  "description": "api-cli visual tool",
  "devtools_page": "devtools.html",
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo-32.png"
  },
  "background": {
    "page": "background.html"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "bookmarks",
    "cookies",
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
  ]
}
